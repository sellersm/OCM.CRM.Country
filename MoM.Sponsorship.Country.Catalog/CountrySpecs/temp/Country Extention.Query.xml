    dbo.USR_COUNTRYEXTENTION
    left join dbo.CHANGEAGENT as [ADDEDBY] on [ADDEDBY].ID = USR_COUNTRYEXTENTION.ADDEDBYID
    left join dbo.CHANGEAGENT as [CHANGEDBY] on [CHANGEDBY].ID = USR_COUNTRYEXTENTION.CHANGEDBYID
